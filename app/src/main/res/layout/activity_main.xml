<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.example.propertyanimationexample.valueAnimatorModel.MyAnimView
        android:id="@+id/myAnimView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.example.propertyanimationexample.objectAnimatorModel.MyAnimView2
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.example.propertyanimationexample.interpolatorModel.MyAnimView3
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="属性动画"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>